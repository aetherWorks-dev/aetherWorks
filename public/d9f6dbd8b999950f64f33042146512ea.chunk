 of cd part cdpts into AC_Objexit repeatend ifend repeatend ifif AC_Obj is empty thenrepeat with bgpts = number of bg parts down to 1if (mLoc is within the rect of bg part bgpts)¬and (the second word of the name of bg part bgpts contains ACwhat)¬and ( the visible of bg part bgpts ) thenput "bg,"&ACwhere&","&the id of bg part bgpts&","& the rect of bg part bgpts into AC_Objexit repeatend ifend repeatend ifAC_selectset cursor to arrowexit AddColorClickend ifif AC_Mode=4 thenAddColor getObjectClicked,"bg",mloc,rectTypeput the result into objif obj is not "-1,-1" thenAddColor getObjectBounds,bg,getitem(obj,1)put the result into tempput "bg,rect,"& getitem(obj,1)&","&temp into AC_Objend ifif not (the editBkgnd) thenAddColor getObjectClicked,"cd",mLoc,rectTypeput the result into objif obj is not "-1,-1" thenAddColor getObjectBounds,cd,getitem(obj,1)put the result into tempput "cd,rect,"& getitem(obj,1)&","&temp into AC_Objend ifend ifAC_selectset cursor to arrowexit AddColorClickend ifif AC_Mode=3 thenAddColor getObjectClicked,"bg",mLoc,"pictType"put the result into objput  getitem(obj,1) into layerAddColor getObjectClicked,"bg",mLoc,"pictFileType"put the result into objput  getitem(obj,1) into layer1if layer1 > layer then put layer1 into layerif layer> -1 thenAddColor getObjectBounds,bg,layerput the result into tempput "bg,pict,"& layer & ","& temp into AC_Objend ifif not (the editBkgnd) thenAddColor getObjectClicked,"cd",mLoc,pictTypeput the result into objput  getitem(obj,1) into layerAddColor getObjectClicked,"cd",mLoc,pictFileTypeput the result into objput  getitem(obj,1) into layer1if layer1 > layer then put layer1 into layerif layer> -1 thenAddColor getObjectBounds,cd,layerput the result into tempput "cd,pict,"& layer & ","& temp into AC_Objend ifend ifAC_selectset cursor to arrowexit AddColorClickend ifend AddColorClickon AddColorColor ColorR,ColorG,ColorBglobal AC_RGB,AC_Objput ColorR&","