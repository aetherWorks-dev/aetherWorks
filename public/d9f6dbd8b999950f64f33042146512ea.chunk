 menuitem "Cut" of menu "Edit"disable menuitem "Copy" of menu "Edit"disable menuitem "Paste" of menu "Edit"create menu "Items"put "Bring To Front"&return&"Bring Closer"&return&¬"Send Farther"&return&"Send To Back"&return&"-"&return&"Place Picture..."&return&"Create New Picture"&return&"Edit Picture..."&return&¬"Place Rectangle"&return&"-"&return&"Get Info..." into menu "Items"¬with menuMsg AC_Front,AC_Closer,AC_Farther,AC_Back,"",AC_NewPict,AC_NewPicture,AC_EditPICT,AC_NewRect,"",AddColorDoubleClickcreate menu "Effects"put "Stack Transitions..."&return&"Background Transitions..."&return&"Card Transitions..." into menu¬"Effects" with menuMsg AC_SetStackTrans,AC_SetBgTrans,AC_SetCdTransif there is a menu "Color" then delete menu "Color"create menu "Color"put "Close Coloring Tools"&return&"Redraw Screen" into menu "Color"¬with menuMsg AC_start,AC_Redrawset menuMsg of menuItem "Close Coloring Tools" of menu "Color"¬to "AddColorCloseM"set the cmdChar of menuItem "Undo" of menu "Edit" to "Z"set the cmdChar of menuItem "Cut" of menu "Edit" to "X"set the cmdChar of menuItem "Copy" of menu "Edit" to "C"set the cmdChar of menuItem "Paste" of menu "Edit" to "V"set the cmdChar of menuItem "Background" of menu "Edit" to "B"set the cmdChar of menuItem "Bring Closer" of menu "Items" to "K"set the cmdChar of menuItem "Send Farther" of menu "Items" to "L"set the cmdChar of menuItem "Get Info..." of menu "Items" to "I"-- Localize --∆AC_DimMenusput 1 into AC_Modeput empty into AC_Objput "13107,13107,26214" into AC_RGBput empty into AC_Undoput true into AC_Runningput the userlevel into AC_Userset the userlevel to 3put the second word of the long name of this stack into pathnameput char 2 to (length(pathname)-1) of pathname into pathnameput resources( pathname ) into ResThisStackif "Time Stamp" is not in ResThisStack thensend "AC_Transitions "&quote&stack&quote&","&quote&"stamp,0"&quote&","&quote&¬"Start"&quote to this cardend ifeditor installAddColor installput the result into errAC_errorChk "Start",errAddColor enableAC_ColorAddColorpaletteif AC_Palminder is not empty thenput the top of card window into y1put the left of card window into x1put getitem(AC_Palminder,1)-(x1)&","&getitem(AC_Palminder,2)-(y1) into AC_Palminder--answer AC_Palminderset the loc of window "AddColor" to AC_Palminderend ifshow window "AddColor"unlock screenset the cursor to arrowclicker initelsebeepend ifset lockscreen to falseend AC_start-- Every line of AC_DimMenus&AC_LightMenus needs localization  --∆on AC_DimMenusdisable menuitem "Bring To Front" of menu "Items"disable menuitem "Bring Closer" of menu "Items"disable menuitem "Send Farther" of menu "Items"disable menuitem "Send To Back" of menu "Items"disable menuitem "Cut" of menu "Edit"disable menuitem "Copy" of menu "Edit"disable menuitem "Edit Picture..." of menu "Items"disable menuitem "Get Info..." of menu "Items"end AC_DimMenuson AC_LightMenusglobal AC_Objenable menuitem "Bring To Front" of menu "Items"enable menuitem "Bring Closer" of menu "Items"enable menuitem "Send Farther" of menu "Items"enable menuitem "Send To Back" of menu "Items"enable menuitem "Cut" of menu "Edit"enable menuitem "Copy" of menu "Edit"if getitem(AC_Obj,2) is "pict" thenenable menuitem "Edit Picture..." of menu "Items"end ifenable menuitem "Get Info..." of menu "Items"end AC_LightMenus-- New version of Compact that removes HCbg and HCcd resources of cards-- That no longer exist.on AC_Compactglobal AC_Undo--disable menuItem "Undo" in menu "Edit" --∆put empty into AC_Undoset cursor to watchlock screenlock messageslock recent-- Cleans up resources of exixsting cardsput the number of this card into yput the number of cards into xrepeat xAddColor compact,"cd"AddColor compact,"bg"go next cardend repeatgo to card y--get list of res's and figure out if the objects existput the second word of the long name of this stack into pathnameput char 2 to (length(pathname)-1) of pathname into pathnameput resources( pathname ) into resListput empty into dataBaseList-- pare the list down to Data base items onlyrepeat the number of lines in resList timesif line 1 of reslist contains "HCcd" or line 1 of reslist contains "HCbg" thenput line 1 of resList&return after dataBaseListend ifdelete line 1 of resListend repeatput empty into resToKillrepeat the number of lines in dataBaseList timesif line 1 of dataBaseList contains "HCcd" thenif there is not a cd id (word 4 of line 1 of dataBaseList) thenput "CD"&&word 2 of line 1 of dataBaseList&return after resToKillend ifelseif there is not a bg id (word 4 of line 1 of dataBaseList) thenput "BG"&&word 2 of line 1 of dataBaseList&return after resToKillend ifend ifdelete line 1 of dataBaseListend repeatrepeat the number of lines in resToKill timesif line 1 of resToKill contains "CD" thenput "HCcd" into ResTypeelseput "HCbg" into ResTypeend ifResRemove pathname, ResType, word 2 of line 1 of resToKilldelete line 1 of resToKillend repeatunlock screenunlock recentunlock messagesend AC_Compacton AC_RemoteInstall scriptsif (scripts is empty) or (scripts is "True") thensend "AC_Transitions "&quote&stack&quote&","&quote&"stamp,0"&quote&","&quote&¬"Start"&quote to this cardAC_DoPassesend ifeditor installAddColor installput the result into errAC_errorChk "AC_RemoteInstall",errAddColor enableAddColor colorCardend AC_RemoteInstallon AC_SaveMem onoffglobal AC_OffBitDepthif onoff then-- New, undocumented feature!addcolor depthput the result into AC_OffBitDepthaddcolor removeelseaddcolor install,AC_OffBitDepthaddcolor colorcardend ifend AC_SaveMemon AC_EditPICTglobal AC_Obj, AC_KillBuf, AC_LowMemhide window "ADDcolor"AddColor "getPictName",getitem(AC_Obj,1),getitem(AC_Obj,3)put the result into pictAddColor "getObjectType",getitem(AC_Obj,1),getitem(AC_Obj,3)put the result into pTypeif pType contains "file" thenput "F" into pTypeelseput "R" into pTypeend ifif pType is "R" thenAddColor IsDuplicatePicture,pictput the result into DuplicatePictelseAddColor IsDuplicatePictureFile,pictput the result into DuplicatePictend ifput true into callAddPaintif AC_LowMem is empty thenAC_DoBuff AC_CanCallAddPaintif the result is true thenAC_DoBuff AC_NeedToSaveMemput the result into AC_KillBufput true into AC_LowMemelseput false into callAddPaintend ifend ifif callAddPaint thenif AC_KillBuf is true then AC_SaveMem TRUEAddPaint pict,ptype,duplicatepictput the result into JuggleNamesif AC_KillBuf is true then AC_SaveMem FALSEif JuggleNames is not empty thenAddColor "getObjectColor",getitem(AC_Obj,1),getitem(AC_Obj,3)put the result into OpacityAddColor "removeObject",getitem(AC_Obj,1),getitem(AC_Obj,3)if pType is "R" thenAddColor "addPict",getitem(AC_Obj,1),JuggleNames,¬getitem(AC_Obj,4)&","&getitem(AC_Obj,5)&","&getitem(AC_Obj,6)&","&getitem(AC_Obj,7),Getitem(Opacity,4),(getitem(AC_Obj,3)-1)put the result into showreselseAddColor "addPictFile",getitem(AC_Obj,1),JuggleNames,¬getitem(AC_Obj,4)&","&getitem(AC_Obj,5)&","&getitem(AC_Obj,6)&","&getitem(AC_Obj,7),Getitem(Opacity,4),(getitem(AC_Obj,3)-1)end ifelseeditor getpictrect, pType, pictput the result into slopend ifend ifset the mode of window "ADDcolor" to 3addcolormode 3show window "ADDcolor"AddColor colorcardAC_Selectend AC_EditPICTon AC_NewPictureglobal AC_KillBuf, AC_LowMemput true into callAddPaintif AC_LowMem is empty thenAC_DoBuff AC_CanCallAddPaintif the result is true thenAC_DoBuff AC_NeedToSaveMemput the result into AC_KillBufput true into AC_LowMemelseput false into callAddPaintend ifend ifif callAddPaint thenhide window "ADDcolor"if AC_KillBuf is true then AC_SaveMem TRUEADDpaint ""if AC_KillBuf is true then AC_SaveMem FALSEset the mode of window "AddColor" to 3addcolormode 3show window "ADDcolor"end ifend AC_NewPictureon AddColorPasteglobal AC_Edit,AC_Objput getitem(AC_Edit,1) into objif the editBkgnd thenput "bg" into layerelseput "cd" into layerend ifif obj is "P" thenput catstr(getitem(AC_Edit,3),getitem(AC_Edit,4),getitem(AC_Edit,5),getitem(AC_Edit,6))¬into boundsAddColor "addPict",layer,getitem(AC_Edit,2),bounds,getitem(AC_Edit,7),-1set the mode of window "AddColor" to 3AddcolorMode 3Addcolorclick getitem(AC_Edit,3),getitem(AC_Edit,4)else if obj is "R" thenput catstr(getitem(AC_Edit,2),getitem(AC_Edit,3),getitem(AC_Edit,4),getitem(AC_Edit,5))¬into boundsput catstr(getitem(AC_Edit,6),getitem(AC_Edit,7),getitem(AC_Edit,8))¬into RGBAddColor "addRect",layer,bounds,RGB,getitem(AC_Edit,9),-1set the mode of window "AddColor" to 4AddcolorMode 4Addcolorclick getitem(AC_Edit,2),getitem(AC_Edit,3)else if obj is "B" thenif getitem(AC_Obj,2) = "btn" thenput catstr(getitem(AC_Edit,2),getitem(AC_Edit,3),getitem(AC_Edit,4))¬into RGBAddColor "addButton",getitem(AC_Obj,1),getitem(AC_Obj,3),RGB,getitem(AC_Edit,5),-1end ifelse if obj is "F" thenif getitem(AC_Obj,2) = "fld" thenput catstr(getitem(AC_Edit,2),getitem(AC_Edit,3),getitem(AC_Edit,4))¬into RGBAddColor "addField",getitem(AC_Obj,1),getitem(AC_Obj,3),RGB,getitem(AC_Edit,5),-1end ifend ifend AddColorPasteon AddColorCopyglobal AC_Obj,AC_Editput getitem(AC_Obj,2) into objput getitem(AC_Obj,1) into layerput getitem(AC_Obj,3) into index_IDif obj is "pict" thenAddColor "getPictName",layer,index_IDput "P,"&the result&"," into AC_EditAddColor "getObjectBounds",layer,index_IDput the result&"," after AC_EditAddColor "getObjectColor",layer,index_IDput the last item of the result after AC_Editelse if obj is "rect" thenAddColor "getObjectBounds",layer,index_IDput "R,"&the result&"," into AC_EditAddColor "getObjectColor",layer,index_IDput the result&"," after AC_EditAddColor "getObjectBevel",layer,index_IDput the result after AC_Editelse if obj is "btn" thenAddColor "getButtonIndex",layer,index_IDput the result into index_IDAddColor "getObjectColor",layer,index_IDput "B,"&the result&"," into AC_EditAddColor "getObjectBevel",layer,index_IDput the result after AC_Editelse if obj is "fld" thenAddColor "getFieldIndex",layer,index_IDput the result into index_IDAddColor "getObjectColor",layer,index_IDput "F,"&the result&"," into AC_EditAddColor "getObjectBevel",layer,index_IDput the result after AC_Editend ifenable menuitem "Paste" of menu "Edit"end AddColorCopy--Trap all handler messages stored in resourse fork of stackon AC_DoPassesAC_doBuff AC_DoPassesend AC_DoPasseson AddColorCardChangeAC_doBuff AddColorCardChangeend AddColorCardChangeon AddColorStackChangeAC_doBuff AddColorStackChangeend AddColorStackChangeon AC_SetUndoColorAC_doBuff AC_SetUndoColorend AC_SetUndoColoron AC_UndoAC_doBuff AC_Undoend AC_Undoon AC_SortAC_doBuff AC_Sortend AC_Sorton AC_CheckMemAC_doBuff AC_CheckMemend AC_CheckMemon AC_NewRectAC_doBuff AC_NewRectend AC_NewRecton AC_NewPictAC_doBuff AC_NewPictend AC_NewPicton AC_PictFileAC_doBuff AC_PictFileend AC_PictFileon AddColorDoubleClickAC_doBuff AddColorDoubleClickend AddColorDoubleClickon AC_ChkStkif the short name of this stack is "Color Tools" then --∆beepanswer "You can not Color this stack.  Please use another." --∆exit to hypercardend ifend AC_ChkStkon AddColorCloseMAC_doBuff AddColorCloseMend AddColorCloseMon AC_ColorAC_doBuff AC_Colorend AC_Coloron AddColorDeleteAC_doBuff AddColorDeleteend AddColorDelete-- get routine from 32K buffer and runon AC_DoBuff handlernameglobal AC_64put offset("on"&&handlername,AC_64) into startptput offset("end"&&handlername,AC_64)+length("end"&&handlername)-1¬into endptput char startpt to endpt of AC_64 into routinedo routineif the result is not empty then return the resultend AC_DoBuffon AC_Pass scripttypedo "put the script of this "&scripttype&"  into scriptVar"put Add_Pass ("openCard",scriptVar) into scriptvarput Add_Pass ("openStack",scriptVar) into scriptvarput Add_Pass ("closeCard",scriptVar) into scriptvarput Add_Pass ("closeStack",scriptVar) into scriptvardo "set the script of this "&scripttype&"  to scriptVar"end AC_Passon keydown keydglobal AC_Runningif AC_Running is empty thenput false into AC_Runningend ifif AC_Running thenput ChartoNum(keyd) into varif var > 27 and var < 32 thenpass keydownend ifelsepass keydownend ifend keydownon AC_Move whereglobal AC_Obj,AC_Undodisable menuItem "Undo" in menu "Edit" --∆put empty into AC_Undoif AC_Obj is not empty thenput getitem(AC_Obj,2) into objput getitem(AC_Obj,1) into layerput getitem(AC_Obj,3) into index_IDif obj="btn" thenAddColor getButtonIndex,layer,index_IDput the result into index_IDelse if obj="fld" thenAddColor getFieldIndex,layer,index_IDput the result into index_IDend ifdo "AddColor "&&where&&",layer,index_ID"put the result into index_IDif (obj = "pict") or (obj="rect") thenput layer&","& getitem(AC_Obj,2)&","&index_ID into AC_Objend ifAC_Colorend ifend AC_Moveon AC_SetTransitions typeglobal AC_Undodisable menuitem "Undo" in menu "Edit"put empty into AC_Undoeditor getTransitionif the result is not empty thenput the result into trantypedo "put the script of this "&type&" into scriptvar"put checkInHandler("colorMe", "AddColor colorCard,"&trantype, scriptvar,"true") into scriptvardo "set the script of this "&type&" to scriptvar"end ifend AC_SetTransitionsfunction AC_CheckInstallput the second word of the long name of this stack into pathnameput char 2 to (length(pathname)-1) of pathname into pathnameput resources( pathname ) into ResThisStackif "Time Stamp" is in ResThisStack thenreturn trueend ifif ("on colorMe" is not in the script of this stack) and¬("on colorMe" is not in the script of this card) and¬("on colorMe" is not in the script of this bg) thenanswer "Opening the Color Editor will install scripts and resources"&" "¬&"in your stack to allow it to display color objects." with "OK" or "Cancel" --∆if it is "Cancel" then return false --∆end ifreturn trueend AC_CheckInstallon AC_Transitions scripttype,trantype,whoif trantype is not empty thendo "put the script of this "&scripttype&" into scriptvar"put checkInHandler("openCard", "Send colorMe to this card", scriptvar) into scriptvarput checkInHandler("closeCard", "lock screen", scriptvar) into scriptvarif ((who = "Start") and ("on colorMe" is not in scriptvar)) or (who <>"Start") thenput checkInHandler("colorMe", "AddColor colorCard,"&trantype, scriptvar, true) into scriptvarend ifput checkInHandler("openStack", "AddColor install", scriptvar) into scriptvarput 	checkInHandler("closeStack", "AddColor remove", scriptvar) into scriptvardo "set the script of this "&scripttype&" to scriptvar"AC_DoPassesend ifend AC_Transitionson doMenu menuglobal AC_Obj,AC_Runningif AC_Running is true thenif menu is "Background"then --∆if getitem(AC_Obj,1)="cd" thenAC_Dselectput empty into AC_Objend ifsend "doMenu Background" to HyperCard --∆set the checkmark of menuItem "Background" of menu "Edit" to the editBkgnd --∆AC_Colorelse if menu is "Quit HyperCard" then --∆clicker quitpass doMenu-- all of the Menuitems listed below, localize --∆else if menu is "Undo" thenAC_Undoelse if menu is "Cut" thenAddColorDeleteelse if menu is "Copy" thenAddColorCopyelse if menu is "Paste" thenAddColorPasteelse if menu is "Bring Closer" thenAC_Move "moveForward"else if menu is "Send Farther" thenAC_Move "moveBackward"else if menu is "Bring To Front" thenAC_Move "moveToFront"else if menu is "Send To Back" thenAC_Move "moveToBack"else if menu is "Compact Stack" thendoNotMenuelse if menu is "Protect Stack..." thendoNotMenuelse if menu is "Delete Stack..." thendoNotMenuelse if menu is "Print Card" thendoNotMenuelse if menu is "Print Stack..." thendoNotMenuelse if menu is "Close Stack" thendoNotMenuelsepass doMenuend ifelsepass doMenuend ifend doMen