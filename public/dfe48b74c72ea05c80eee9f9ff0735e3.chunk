- sectorError: (char const *)msg : (int)line1 : (int)line2
{
	[pan	orderOut:NULL];
	NXFreeAlertPanel	(pan);
	[editworld_i deselectAll];
	if (line1 != -1)
	{
		[editworld_i selectLine: line1];
		[editworld_i selectPoint: lines[line1].p1];
